<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<g:VerticalPanel>
		<g:HorizontalPanel>
			<g:HTML>
				Dane użytkownika:
				<br />
			</g:HTML>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:HTML>Imię:</g:HTML>
			<g:Label ui:field="labelFirstName"></g:Label>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:HTML>Nazwisko:</g:HTML>
			<g:Label ui:field="labelLastName"></g:Label>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:HTML>Średnia ocena:</g:HTML>
			<g:Label ui:field="labelEvaluation"></g:Label>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:HTML>E-mail:</g:HTML>
			<g:Label ui:field="labelEmail"></g:Label>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:HTML>Płeć:</g:HTML>
			<g:Label ui:field="labelSex"></g:Label>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:HTML>Adres korespondecyjny:</g:HTML>
			<g:Label ui:field="labelAddress"></g:Label>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:HTML>Miasto:</g:HTML>
			<g:ListBox ui:field="cities" />
			<g:ListBox ui:field="regions" />
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:HTML>Kod pocztowy:</g:HTML>
			<g:Label ui:field="labelPostalCode"></g:Label>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:HTML>Numer stacjonarny:</g:HTML>
			<g:Label ui:field="labelPhoneNumber"></g:Label>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:HTML>Numer komorkowy:</g:HTML>
			<g:Label ui:field="labelMobile"></g:Label>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:HTML>Specializacje:</g:HTML>
			<g:Label ui:field="labelSpecializations"></g:Label>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:Button ui:field="buttonDoctorHomePage">Strona domowa lekarza</g:Button>
			<g:Button ui:field="buttonDisplayCalendar">Kalendarz</g:Button>
			<g:Button ui:field="buttonCloseWindow">Zamknij</g:Button>
		</g:HorizontalPanel>
	</g:VerticalPanel>


</ui:UiBinder>